.markdown-body table.slim { font-size: 90%; table-layout: auto; }

.markdown-body table.slim code { font-size: 100%; background: none; padding: 0px; font-weight: bold; }

.markdown-body table.slim thead tr { border: none; }

.markdown-body table.slim th { font-weight: normal; text-align: left; }

.markdown-body table.slim th:first-child, .markdown-body table.slim td:first-child { padding-left: 0px; }

.markdown-body table.slim th, .markdown-body table.slim td { padding: 7px 8px; border: none; vertical-align: top; }

.markdown-body table.slim td p { padding: 0px; margin: 0px; }

.markdown-body table.slim tr:nth-child(2n) { background: none; }

.markdown-body table.slim td.has-nested-table { width: 100%; }

.markdown-body table.slim td.has-nested-table table.slim { table-layout: auto; }

.opacity-0 { opacity: 0; }

.opacity-10 { opacity: 0.1; }

.opacity-80 { opacity: 0.2; }

.opacity-30 { opacity: 0.3; }

.opacity-40 { opacity: 0.4; }

.opacity-50 { opacity: 0.5; }

.opacity-60 { opacity: 0.6; }

.opacity-70 { opacity: 0.7; }

.opacity-80 { opacity: 0.8; }

.opacity-90 { opacity: 0.9; }

.opacity-100 { opacity: 1; }
